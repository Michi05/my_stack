<launch>

  <!-- Gazebo -->
  <include file="$(find my_gazebo_worlds)/launch/gazebo.launch" >
    <arg name="world" value="$(find my_gazebo_worlds)/worlds/empty.world" />
  </include>

  <!-- Walls -->
  <node
    name="spawn_walls"
    pkg="gazebo"
    type="spawn_model"
    args="-gazebo -model walls -file $(find my_gazebo_worlds)/models/peishome.model"
    respawn="false"
    output="screen"
  />

</launch>
